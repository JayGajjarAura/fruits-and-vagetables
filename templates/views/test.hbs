                    <div>
                        <div class="text-center">
                            <button value="-" id="{{this._id}}" class="value_btn minus_btn"> - </button>
                            <input disabled id="qty_{{this._id}}" class="input_area mx-1 text-center input_val" value="0">
                            <button value="+" id="{{this._id}}" class="value_btn mb-3 plus_btn"> + </button>
                        </div>
                        <button class="add_to_cart_toast add_to_cart_btn w-100 py-1 mb-2"> Add to cart </button>
