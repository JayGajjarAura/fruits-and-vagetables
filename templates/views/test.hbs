    <div class="row row-cols-1 row-cols-md-5">
        {{#each products}}
        <div class="col p-2">
            <div class="card">
                <img class="card-img-top mt-3 product_images" src="{{this.image}}" alt="Card image cap" height="180px">
                <div class="card-body px-4 py-1">
                    <p class="card-title font-weight-bold" id="product_red_carot"> {{this.title}} </p>
                    <div class="star_rating_size ">
                        <i class="fa fa-star"></i>
                        <i class="fa fa-star"></i>
                        <i class="fa fa-star"></i>
                        <i class="fa fa-star"></i>
